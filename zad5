CREATE TABLE Polzovatel(
    id int(10) not null PRIMARY KEY,
    Name varchar(50),
    Familiya varchar(50),
    Otchestvo varchar(50),
    Bithday date)
    
    CREATE TABLE category(
    id_category int not null PRIMARY KEY,
    category varchar(50),
    poditel_category int);

    CREATE TABLE teg(
    id int not null PRIMARY KEY,
    teg varchar(50),
    color varchar(20));

   CREATE TABLE proect(
    id int not null PRIMARY KEY,
    proekt varchar(50));
    
    CREATE TABLE Zadacha(
    id int(10) not null PRIMARY KEY,
    Name varchar(50),
    Nachalo date,
    Konez date,
    proekt int(50) ,
    kategoriya int(50));
